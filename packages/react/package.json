{
  "name": "@trilogy-ds/react",
  "version": "0.0.1-beta.78.2",
  "author": "Bouygues Telecom",
  "files": [
    "lib"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "typings": "lib/esm/index.d.ts",
  "sideEffect": false,
  "description": "Trilogy react framework design system for Bouygues Telecom",
  "license": "UNLICENSED",
  "scripts": {
    "test": "jest --config jest.config.web.ts --updateSnapshot --coverage",
    "check-types": "tsc --noemit",
    "test:native": "jest --config jest.config.native.ts --updateSnapshot --coverage --silent=false",
    "prebuild": "node -p \"'export const version = ' + JSON.stringify(require('./package.json').version)\" > version.ts",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "rimraf lib && npm run prebuild && npm run build:esm && npm run build:cjs && tsc-alias && cp -R ../../config/native/react-native.config.js ./lib/"
  },
  "dependencies": {
    "@ptomasroos/react-native-multi-slider": "^2.2.2",
    "@react-native-picker/picker": "^2.6.1",
    "@trilogy-ds/assets": "*",
    "@trilogy-ds/styles": "*",
    "clsx": "^2.1.0",
    "color": "^4.2.3",
    "react-content-loader": "^6.2.1",
    "react-native-gesture-handler": "^2.16.0",
    "react-native-modal": "^13.0.1",
    "react-native-toast-message": "^2.2.0",
    "rollup-plugin-copy": "^3.5.0",
    "shortid": "^2.2.16"
  },
  "keywords": [
    "react",
    "design system"
  ],
  "devDependencies": {
    "typescript-transform-paths": "^3.5.2"
  },
  "exports": {
	".": {
		"import": "./lib/esm/index.js",
		"require": "./lib/cjs/index.js",
		"types": "./lib/esm/index.d.ts"
	},
	"./components": {
		"import": "./lib/esm/components/index.js",
		"require": "./lib/cjs/components/index.js",
		"types": "./lib/esm/components/index.d.ts"
	},
	"./components/*": {
		"import": "./lib/esm/components/*/index.js",
		"require": "./lib/cjs/components/*/index.js",
		"types": "./lib/esm/components/*/index.d.ts"
	},
	"./components/enums": {
		"import": "./lib/esm/components/enums.js",
		"require": "./lib/cjs/components/enums.js"
	},
	"./context": {
		"import": "./lib/esm/context/index.js",
		"require": "./lib/cjs/context/index.js"
	},
	"./context/*": {
		"import": "./lib/esm/context/*.js",
		"require": "./lib/cjs/context/*.js"
	},
	"./events": {
		"import": "./lib/esm/events/index.js",
		"require": "./lib/cjs/events/index.js"
	},
	"./helpers": {
		"import": "./lib/esm/helpers/index.js",
		"require": "./lib/cjs/helpers/index.js"
	},
	"./objects": {
		"import": "./lib/esm/objects/index.js",
		"require": "./lib/cjs/objects/index.js"
	},
	"./services": {
		"import": "./lib/esm/services/index.js",
		"require": "./lib/cjs/services/index.js"
	}
  }
}
